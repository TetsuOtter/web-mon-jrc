{
	"compilerOptions": {
		"target": "ES2024",
		"module": "ES2022",
		"moduleResolution": "bundler",
		"lib": ["ES2024"],
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"noUncheckedIndexedAccess": false,
		"noPropertyAccessFromIndexSignature": false
	},
	"include": ["*.ts"],
	"exclude": ["node_modules", "dist"]
}
